{
    "name": "ahmadyousefdev/automs",
    "description": "An auto management system model generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Yousef",
            "email": "ahmedyousefdev@gmail.com",
            "homepage": "https://ahmadyousef.me"
        }
    ],
    "homepage": "https://github.com/ahmadyousefdev/automs",
    "keywords": ["Laravel", "Automs"],
    "require": {
        "illuminate/support": "self.version",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.1",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5|^6"
    },
    "autoload": {
        "psr-4": {
            "Ahmadyousefdev\\Automs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahmadyousefdev\\Automs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahmadyousefdev\\Automs\\AutomsServiceProvider"
            ],
            "aliases": {
                "Automs": "Ahmadyousefdev\\Automs\\Facades\\Automs"
            }
        }
    }
}
